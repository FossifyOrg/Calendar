<?xml version="1.0" encoding="utf-8"?>
<appwidget-provider xmlns:android="http://schemas.android.com/apk/res/android"
    android:configure="org.fossify.calendar.activities.WidgetWeeklyConfigureActivity"
    android:initialLayout="@layout/fragment_week_widget"
    android:minWidth="@dimen/min_widget_width"
    android:minHeight="@dimen/min_widget_height"
    android:minResizeWidth="110dp"
    android:minResizeHeight="110dp"
    android:previewImage="@drawable/img_widget_weekly_preview"
    android:resizeMode="horizontal|vertical"
    android:targetCellWidth="4"
    android:targetCellHeight="4"
    android:updatePeriodMillis="3600000" />
